{
  // https://v3.nuxtjs.org/concepts/typescript
  "extends": "./.nuxt/tsconfig.json",
  "lib": ["ES2019"],
  "compilerOptions": {
    "types": ["@pinia/nuxt"],
    "allowJs": true,
    "paths": {
      "@/*": ["*"]
    },
    "sourceMap": true,
    "inlineSourceMap": true
  },
  "parserOptions": {
    "extraFileExtensions": [".vue"]
  }
}
